<h1>Books#index</h1>
<p>Find me in app/views/books/index.html.erb</p>
<h1>List of Books</h1>

<table border="1">
	<tr>
		<th>id</th>
		<th>Book_name</th>
		<th>author</th>
		<th>publication</th>
		<th>created at</th>
		<th>updated at</th>
		      
	</tr>
	
	<% @books.each do |b|%>
	
	<tr>
		<td><%=  b.id%></td>
		<td><%=  b.book_name %></td>
		<td><%=  b.author %></td>
		<td><%=  b.publication %></td>
		<td><%=  b.created_at %></td>
		<td><%=  b.updated_at %></td>
		<td><%=  link_to 'show', book_show_path(b) %></td>
<td><%=  link_to 'edit', book_edit_path(b) %></td>
<td><%= link_to 'delete', book_destroy_path(b), method: :delete,  data: {confirm: 'are you sure?'} %></td>
	      
	</tr>
	<% end %>
</table>
<td><%= link_to 'New record', book_new_path %></td>



